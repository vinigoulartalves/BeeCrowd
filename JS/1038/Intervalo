var input = require('fs').readFileSync('stdin', 'utf8');
var lines = input.split(' '); 

var codigo = parseFloat(lines[0])
var quant = parseFloat(lines[1])
var preco = []

if (codigo === 1) {
    preco = 4.00
} 
else if (codigo === 2) {
    preco = 4.50
}
else if (codigo === 3) {
    preco = 5.00
}
else if (codigo === 4) {
    preco = 2.00
}
else if (codigo === 5) {
    preco = 1.50
}

console.log(`Total: R$ ${(quant * preco).toFixed(2)}`)